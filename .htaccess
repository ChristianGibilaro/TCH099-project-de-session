RewriteEngine On
RewriteCond %{REQUEST_URI} !(\.png|\.jpg|\.webp|\.gif|\.jpeg|\.zip|\.css|\.svg|\.js|\.pdf|\.mp4|\.mov|\.avi|\.mkv)$

RewriteCond %{REQUEST_URI} !^/ressources/images/profile/
RewriteCond %{REQUEST_URI} !^/ressources/images/team/
RewriteCond %{REQUEST_URI} !^/ressources/videos/

<IfModule mod_headers.c>
    Header set Access-Control-Allow-Origin "*"
    Header set Access-Control-Allow-Methods "GET, POST, PUT, DELETE, OPTIONS"
    Header set Access-Control-Allow-Headers "Content-Type, Authorization"
    Header set Access-Control-Max-Age "3600"

    php_flag display_errors on
    php_value error_reporting E_ALL
</IfModule>
RewriteRule (.*) routes.php [QSA,L]